        },
        
       "clients" : {
           "http://%authority%%contextRoot%/samplecontainer/examples/oauth2.xml" : {
                "redirectURI" : "http://%authority%%contextRoot%/gadgets/oauth2callback",
                "type" : "confidential",
                "flow" : "authorization_code",
                "consumer_key" : "gadgetConsumer",
                "consumer_secret" : "gadgetSecret"
           }
       }
   }   
}  
  

